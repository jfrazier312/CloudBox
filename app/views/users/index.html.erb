<% provide(:title, 'All Users') %>

<div class="center jumbotron">
  <h1>Users</h1>
</div>

<div><% if is_admin? %>
      <%= link_to "Create New Account", new_user_path, :class => "btn btn-primary" %>
  <% end %></div>

<%= will_paginate %>

<ul class="users">
  <table class="table table-striped">
    <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Privilege</th>
      <th>Delete</th>
    </tr>
    </thead>
    <%= render @users %>
  </table>
</ul>

<%= will_paginate %>
