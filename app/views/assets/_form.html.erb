<%= simple_form_for(@asset, :html => { class: 'form-horizontal', multipart: true } ) do |f| %>
    <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :uploaded_file %>
  </div>

  <div class="form-actions">
    <%= f.button :submit, input_html: { class: "btn btn-block" } %>
  </div>
<% end %>
